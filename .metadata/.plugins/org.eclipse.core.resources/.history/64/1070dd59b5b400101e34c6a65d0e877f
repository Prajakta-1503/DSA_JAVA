package com.demo.service;

public class ArrayService {
	private int []arr;
	private int count;
	
	public ArrayService() {
		arr = new int[10];
		count = 0;
	}
	public ArrayService(int [] arr,int count) {
		this.arr=arr;
		this.count=count;
	}
	public ArrayService( int size) {
		arr= new int[size];
		count=0;
		
	}
	public int getCapacity() {
		return arr.length;
	}
	
	public int getSize() {
		return count;
	}

}
